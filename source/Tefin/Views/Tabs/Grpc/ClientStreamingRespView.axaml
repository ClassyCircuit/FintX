<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:grpc="clr-namespace:Tefin.ViewModels.Tabs.Grpc"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:DataType="grpc:ClientStreamingRespViewModel"
             x:Class="Tefin.Views.Tabs.Grpc.ClientStreamingRespView">

    <Border>
        <Grid>
            <ContentControl Content="{Binding ResponseEditor}" />
            <TextBlock Text="response" FontSize="20" Opacity="0.15"
                       IsHitTestVisible="False"
                       VerticalAlignment="Bottom" HorizontalAlignment="Right"
                       Margin="10" />
        </Grid>


    </Border>
</UserControl>